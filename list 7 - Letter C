using System;

namespace lista_6
{
	class Program
	{
		public static void Main(string[] args)
		{
			Console.WriteLine("digite um numero inteiro para o tamanho do vetor ");
			int num = Convert.ToInt32(Console.ReadLine());
			int i = 0;
			int[] jojo = new int[num];
			int soma = 0;
			while(i < num){
				Console.WriteLine("digite um numero");
				jojo[i] = Convert.ToInt32(Console.ReadLine());
				soma = soma + jojo[i];
				i++;
			}
			Console.WriteLine(soma);
			Console.Write("Press any key to continue . . . ");
			Console.ReadKey(true);
		}
	}
}
