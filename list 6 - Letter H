using System;

namespace Atividade_5__H
{
	class Program
	{
		public static void Main(string[] args)
		{
			Console.WriteLine("Digite uma letra: ");
			string letra = (Console.ReadLine().ToLower());
			
			switch (letra) {
					case "a": Console.WriteLine("vogal");break;
					case "e": Console.WriteLine("vogal");break;
					case "i": Console.WriteLine("vogal");break;
					case "o":  Console.WriteLine("vogal");break;
					case "u": Console.WriteLine("vogal");break;
					default: Console.WriteLine("consoante");break;
			}
			
			
		
			Console.Write("Pressione para continuar . . . ");
			Console.ReadKey(true);
		}
	}
}
